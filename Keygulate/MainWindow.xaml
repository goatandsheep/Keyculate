<Window x:Name="MainWindow1" x:Class="Keygulate.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Keygulate" Height="350" Width="550" Visibility="Visible">
    <Grid Width="550" Height="350">
        <Grid.Background>
            <LinearGradientBrush EndPoint="0.5,1" MappingMode="RelativeToBoundingBox" StartPoint="0.5,0">
                <GradientStop Color="#FFCEFFDE"/>
                <GradientStop Color="White" Offset="1"/>
            </LinearGradientBrush>
        </Grid.Background>
        <Image HorizontalAlignment="Left" Height="330" VerticalAlignment="Top" Width="530" ClipToBounds="True" Margin="10,10,0,0"/>
    </Grid>
</Window>
